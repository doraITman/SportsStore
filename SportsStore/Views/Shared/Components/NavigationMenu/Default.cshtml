@model IEnumerable<string>

<a class="btn btn-dark btn-secondary"
asp-action="List"
asp-controller="Product"
asp-route-category="">
    Home
</a>
@foreach (string category in Model){
    <a class="btn btn-dark btn-secondary"
     asp-action="List"
    asp-controller="Product"
    asp-route-category="@category">
    @category
    </a>
}

